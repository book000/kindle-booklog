{
  "name": "kindle-booklog",
  "version": "3.0.0",
  "description": "Get Kindle works purchased at Amazon.co.jp and register them in Booklog.",
  "homepage": "https://github.com/book000/kindle-booklog",
  "bugs": {
    "url": "https://github.com/book000/kindle-booklog/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:book000/kindle-booklog.git"
  },
  "scripts": {
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "clean": "rimraf dist output",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc",
    "fix:prettier": "prettier --write src",
    "preinstall": "npx only-allow pnpm",
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "fix": "run-z fix:prettier,fix:eslint",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.8.76",
    "@book000/node-utils": "1.15.36",
    "@types/node": "22.7.5",
    "@types/tar-stream": "3.1.3",
    "axios": "1.9.0",
    "csv-parse": "5.6.0",
    "eslint": "9.27.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.18.0",
    "eslint-plugin-promise": "7.2.1",
    "iconv-lite": "0.6.3",
    "otplib": "12.0.1",
    "prettier": "3.5.3",
    "puppeteer-core": "24.8.2",
    "run-z": "2.1.0",
    "tar-stream": "3.1.7",
    "tsx": "4.19.4",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
